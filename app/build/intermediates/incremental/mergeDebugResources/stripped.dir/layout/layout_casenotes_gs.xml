<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout

        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        android:padding="3dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_shape_of_the_abdomen"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="3dp"
                android:text="@string/shape_of_the_abdomen"
                android:textColor="@color/ourcolour" />

            <Spinner
                android:id="@+id/spinner_shape_of_the_abdomen"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/abc_spinner_textfield_background_material"
                android:padding="3dp"
                android:spinnerMode="dialog" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_abdomen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/abdomen"
                android:textColor="@color/ourcolour" />


            <Button
                android:id="@+id/abdon_select"
                style="@style/PositiveButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:padding="3dp"
                android:text="@string/select"
                android:textAlignment="center"
                android:textColor="@color/white" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ListView
                    android:id="@+id/listView1"
                    android:layout_width="match_parent"
                    android:layout_height="150dp" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_visible_pulsation1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="@string/visible_pulsations_txt"
                    android:textColor="@color/ourcolour" />


                <kdmc_kumar.Utilities_Others.toggle.LabeledSwitch
                    android:id="@+id/switch_visible_pulsation1"
                    android:layout_width="82dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:padding="3dp"
                    android:textSize="12sp"
                    app:colorBorder="@color/colorPrimary"
                    app:colorOn="@color/colorPrimary"
                    app:on="false"
                    app:textOff="@string/no"
                    app:textOn="@string/yes" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_switch_visible_perist1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="@string/visible_peristalsis"
                    android:textColor="@color/ourcolour" />

                <kdmc_kumar.Utilities_Others.toggle.LabeledSwitch
                    android:id="@+id/switch_visible_perist1"
                    android:layout_width="82dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:padding="3dp"
                    android:textSize="12sp"
                    app:colorBorder="@color/colorPrimary"
                    app:colorOn="@color/colorPrimary"
                    app:on="false"
                    app:textOff="@string/no"
                    app:textOn="@string/yes" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_abdominal_palpation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="3dp"
                android:text="@string/abdominal_palpation_txt"
                android:textColor="@color/ourcolour" />

            <Spinner
                android:id="@+id/spinner_abdominal_palpation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/abc_spinner_textfield_background_material"
                android:padding="3dp"
                android:spinnerMode="dialog" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_splenomegaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/splenomegaly"
                android:textColor="@color/ourcolour" />

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView_splenomegaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/grey_300"
                android:ems="10"
                android:padding="3dp"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_hepatomegaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/hepatomegaly"
                android:textColor="@color/ourcolour" />

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView_hepatomegaly"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/grey_300"
                android:ems="10"
                android:padding="3dp"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_palpable_masses"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="3dp"
                android:text="@string/palpable_masses"
                android:textColor="@color/ourcolour" />

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView_palpable_masses"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/grey_300"
                android:ems="10"
                android:padding="3dp"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_bowel_sounds"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="3dp"
                android:text="@string/bowel_sounds_txt"
                android:textColor="@color/ourcolour" />

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView_bowel_sounds"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/grey_300"
                android:ems="10"
                android:padding="3dp"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_hernial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/hernial"
                android:textColor="@color/ourcolour" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/checkBox_orifices"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    
                    android:padding="3dp"
                    android:text="@string/orifices" />

                <CheckBox
                    android:id="@+id/checkBox_free"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    
                    android:padding="3dp"
                    android:text="@string/free" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/bowel_sounds"
                android:textColor="@color/ourcolour" />

            <Spinner
                android:id="@+id/spinner3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_spleen"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:layout_weight="2"
                    android:text="@string/spleen"
                    android:textColor="@color/ourcolour" />


                <kdmc_kumar.Utilities_Others.toggle.LabeledSwitch
                    android:id="@+id/switch_spleen"
                    android:layout_width="82dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:textSize="12sp"
                    app:colorBorder="@color/colorPrimary"
                    app:colorOn="@color/colorPrimary"
                    app:on="false"
                    app:textOff="@string/txt_normal"
                    app:textOn="@string/txt_enlarged" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/liver"
                    android:textColor="@color/ourcolour" />


                <kdmc_kumar.Utilities_Others.toggle.LabeledSwitch
                    android:id="@+id/switch_liver_normal"
                    android:layout_width="82dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:textSize="12sp"
                    app:colorBorder="@color/colorPrimary"
                    app:colorOn="@color/colorPrimary"
                    app:on="false"
                    app:textOff="@string/txt_normal"
                    app:textOn="@string/txt_enlarged" />

            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_organomegely"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="@string/organomegely_n"
                    android:textColor="@color/ourcolour" />

                <AutoCompleteTextView
                    android:id="@+id/AutoCompleteTextView_organomegely"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/grey_300"
                    android:ems="10"
                    android:padding="5dp"
                    android:singleLine="true" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_free_fluid_in_the_abdomen"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="@string/free_fluid_in_the_abdomen"
                    android:textColor="@color/ourcolour" />

                <AutoCompleteTextView
                    android:id="@+id/AutoCompleteTextView_free_fluid_in_the_abdomen"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/grey_300"
                    android:ems="10"
                    android:padding="5dp"
                    android:singleLine="true" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_distension"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="@string/distension"
                    android:textColor="@color/ourcolour" />

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteTextView_distension"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/grey_300"
                    android:ems="10"
                    android:padding="5dp"
                    android:singleLine="true" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView_tenderness"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="@string/tenderness"
                    android:textColor="@color/ourcolour" />

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteTextView_tenderness"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/grey_300"
                    android:ems="10"
                    android:padding="5dp"
                    android:singleLine="true" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_scrotum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/scrotum"
                android:textColor="@color/ourcolour" />

            <AutoCompleteTextView
                android:id="@+id/AutoCompleteTextView_scrotum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/grey_300"
                android:ems="10"
                android:padding="5dp"
                android:singleLine="true" />
        </LinearLayout>

    </LinearLayout>
</merge>
 
